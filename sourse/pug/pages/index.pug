extends layout/main.pug


include ../blocks/sContent/_sContent.pug
include ../blocks/sContact/_sContact.pug
include ../blocks/sHowItWorks/_sHowItWorks.pug
include ../blocks/sWhatYouGet/_sWhatYouGet.pug
include ../blocks/sAdvantage/_sAdvantage.pug
include ../blocks/sVideo/_sVideo.pug
include ../blocks/sIndicators/_sIndicators.pug
include ../blocks/sMonitoring/_sMonitoring.pug

block variables
	- let  mainPage= null
block title
	- let  title= 'Онлайн-тренинг'
	title= title
block pagecontent

	+header()
		+topLine() 
		+headerBlock()
	+sHowItWorks()
	+regBlock({
		t:`Готовы уже сейчас записаться&nbsp;на&nbsp;обучение?`,
		 btn:`Кликайте и регистрируйтесь!`
	}).regBlock-sm
	+sWhatYouGet()
	+sAdvantage()
	+sVideo()
	+sIndicators()
	+sMonitoring()
	+sForm()
	+sContact()
	+regBlock({
		t:`Мы&nbsp;продумали наш проект таким образом, чтобы каждый активный участник тренинга заработал деньги прямо во&nbsp;время обучения. Вам остаётся только воспользоваться этой возможностью.`,
		 btn:`Регистрируйтесь на&nbsp;тренинг и&nbsp;зарабатывайте!`
	})
	+footer()


	