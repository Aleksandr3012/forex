mixin sIndicators(data)
	// start sIndicators
	+b.SECTION.sIndicators.section#sIndicators&attributes(attributes)
		.container
			+b.section-title
				h2 Показатели торговой системы 5.0&nbsp;в цифрах:

				
			+e.tableWrap
				table
					thead
						tr
							td Стиль<br> торговли
							td Рабочий<br> таймфрейм
							td Предпочтительное время суток для совершения сделок
							td Занятость в&nbsp;день на 1 актив
							td Кол-во сделок по 1&#8209;му активу
							td Соотношение риск/прибыль в&nbsp;одной сделке
							td Процент прибыльных сделок
							td Прогнозируемая доходность в месяц по 1&#8209;му активу при риске 1% на сделку
							td Возможная максимальная просадка по счёту по 1&#8209;му активу при риске 1% на сделку
					tbody
						tr
							td Внутри дня
							td 
								strong M1 – M15
							td C 10 до 20 МСК
							td 3-5 часов
							td 1-3 в день
							td 1/2 – 1/3
							td 60-70%
							td 40-50%
							td 20%
						tr
							td Среднесрок
							td 
								strong H1 – H4
							td C 10 до 20 МСК
							td 30-60 минут
							td 1-3 в день
							td 1/2 – 1/3
							td 60-70%
							td 30-40%
							td 15%
						tr
							td Долгосрок
							td 
								strong D1
							td В любое время
							td 5-10 минут
							td 1-3 в день
							td 1/2 – 1/3
							td 60-70%
							td 20-30%
							td 10%
	// end sIndicators